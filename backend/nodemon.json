{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": false,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "index.js",
    "ormconfig.json",
    "entities",
    "lib",
    "routes"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}